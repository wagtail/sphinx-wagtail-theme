{
  "root": true,
  "extends": ["@wagtail/eslint-config-wagtail"],
  "parserOptions": {
    "ecmaVersion": 2022
  },
  "env": { "browser": true },
  "globals": {
    "DOCUMENTATION_OPTIONS": "readonly",
    "Search": "readonly"
  },
  "rules": {
    "no-plusplus": "off",
    "no-underscore-dangle": ["error", { "allow": ["_index"] }],
    "vars-on-top": "off"
  }
}
